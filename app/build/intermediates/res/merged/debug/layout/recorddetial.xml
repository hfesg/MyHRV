<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#efefef"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#efefef"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:background="#4fa5d5" >

            <Button
                android:id="@+id/fanhui"
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:layout_alignParentLeft="true"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="10dip"
                android:layout_marginTop="10dip"
                android:background="@drawable/backha" />

            <Button
                android:id="@+id/xindian"
                android:layout_width="35dip"
                android:layout_height="35dip"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="5dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="5dip"
                android:background="@drawable/ecglogo" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:background="#4fa5d5"
                android:text="分析报告"
                android:textColor="#ffffff"
                android:textSize="20dip" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="10dp"
            android:background="#ffffff"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="8dp" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:layout_marginLeft="10dip"
                android:gravity="center_vertical"
                android:src="@drawable/ic_action_calendar_day" />

            <TextView
                android:layout_width="60dip"
                android:layout_height="30dip"
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:text="@string/date"
                android:textColor="#00BFFF"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center_vertical"
                android:text="2016年03月01日"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:background="#ffffff"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="8dp" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:layout_marginLeft="10dip"
                android:gravity="center_vertical"
                android:src="@drawable/ic_action_clock" />

            <TextView
                android:layout_width="60dip"
                android:layout_height="30dip"
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:text="@string/time"
                android:textColor="#00BFFF"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center_vertical"
                android:text="19时25分"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:background="#ffffff"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="8dp" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dip"
                android:src="@drawable/ic_action_heart_blue" />

            <TextView
                android:layout_width="60dip"
                android:layout_height="30dip"
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:text="@string/xinlv"
                android:textColor="#00BFFF"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/xinlv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center_vertical"
                android:text="@string/xinlvzhi"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/icon_min_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:text="@string/min_2" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:background="#ffffff"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="8dp" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dip"
                android:src="@drawable/ic_action_font_bold" />

            <TextView
                android:layout_width="60dip"
                android:layout_height="30dip"
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:text="@string/huxi"
                android:textColor="#00BFFF"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/huxi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center_vertical"
                android:text="000"
                android:textSize="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:text="@string/min_2" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:background="#ffffff"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="8dp" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:layout_marginLeft="10dip"
                android:gravity="center_vertical"
                android:src="@drawable/ic_action_emo_angry" />

            <TextView
                android:layout_width="60dip"
                android:layout_height="30dip"
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:text="@string/jiaolv"
                android:textColor="#00BFFF"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/jiaolv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center_vertical"
                android:text="@string/jiaolvzhi"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dip"
            android:orientation="vertical" >

            <TabHost
                android:id="@+id/t1"
                android:layout_width="fill_parent"
                android:layout_height="match_parent" >

                <LinearLayout
                    android:id="@+id/layout1"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:orientation="vertical" >

                    <TabWidget
                        android:id="@android:id/tabs"
                        android:layout_width="match_parent"
                        android:layout_height="50dip" >
                    </TabWidget>

                    <FrameLayout
                        android:id="@android:id/tabcontent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" 
                        android:layout_marginTop="5dp">

                        <LinearLayout
                            android:id="@+id/tabview1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical" >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:background="#ffffff"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:padding="8dp" >

                                <ImageView
                                    android:layout_width="10dip"
                                    android:layout_height="10dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="10dip"
                                    android:src="@drawable/btn_kaishi_1" />

                                <TextView
                                    android:layout_width="80dip"
                                    android:layout_height="30dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="25dip"
                                    android:text="@string/SDNN"
                                    android:textColor="#00BFFF"
                                    android:textSize="23dp" />

                                <TextView
                                    android:id="@+id/SDNN"
                                    android:layout_width="70dip"
                                    android:layout_height="30dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="50dip"
                                    android:text="@string/SDNNZHI"
                                    android:textSize="20dp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:background="#ffffff"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:padding="8dp" >

                                <ImageView
                                    android:layout_width="10dip"
                                    android:layout_height="10dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="10dip"
                                    android:src="@drawable/btn_kaishi_1" />

                                <TextView
                                    android:layout_width="80dip"
                                    android:layout_height="30dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="25dip"
                                    android:text="@string/NN"
                                    android:textColor="#00BFFF"
                                    android:textSize="23dp" />

                                <TextView
                                    android:id="@+id/NN"
                                    android:layout_width="70dip"
                                    android:layout_height="30dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="50dip"
                                    android:text="@string/NNZHI"
                                    android:textSize="20dp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:background="#ffffff"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:padding="8dp" >

                                <ImageView
                                    android:layout_width="10dip"
                                    android:layout_height="10dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="10dip"
                                    android:src="@drawable/btn_kaishi_1" />

                                <TextView
                                    android:layout_width="80dip"
                                    android:layout_height="30dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="25dip"
                                    android:text="@string/PNN"
                                    android:textColor="#00BFFF"
                                    android:textSize="23dp" />

                                <TextView
                                    android:id="@+id/PNN"
                                    android:layout_width="70dip"
                                    android:layout_height="30dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="50dip"
                                    android:text="@string/PNNZHI"
                                    android:textSize="20dp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:background="#ffffff"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:padding="8dp" >

                                <ImageView
                                    android:layout_width="10dip"
                                    android:layout_height="10dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="10dip"
                                    android:src="@drawable/btn_kaishi_1" />

                                <TextView
                                    android:layout_width="80dip"
                                    android:layout_height="30dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="25dip"
                                    android:text="@string/rMSSD"
                                    android:textColor="#00BFFF"
                                    android:textSize="23dp" />

                                <TextView
                                    android:id="@+id/rMSSD"
                                    android:layout_width="70dip"
                                    android:layout_height="30dip"
                                    android:gravity="center_vertical"
                                    android:layout_marginLeft="50dip"
                                    android:text="@string/rMSSDZHI"
                                    android:textSize="20dp" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/tabview2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="#ffffff"
                            android:orientation="vertical" >

                            <LinearLayout
                                android:id="@+id/pinyutu"
                                android:layout_width="match_parent"
                                android:layout_height="180dp"
                                android:layout_marginTop="20dp"
                                android:background="#ffffff"
                                android:orientation="vertical" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/tabview3"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="#ffffff"
                            android:orientation="vertical" >

                            <LinearLayout
                                android:id="@+id/sandiantu"
                                android:layout_width="match_parent"
                                android:layout_height="180dp"
                                android:layout_marginTop="20dp"
                                android:orientation="vertical" />
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </TabHost>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout 
        android:id="@+id/bitmap_layout"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:orientation="vertical"
        ></LinearLayout>

</LinearLayout>